<div class="card object-card">


    <span class="badge badge-primary badge-found"><i class="fa fa-map-pin"></i> {{ traobj.state.label }}</span>
    <img class="card-img-top" src="{{ asset('uploads/' ~ traobj.picture) }}" alt="Portefeuille">
    <div class="card-body">
        <h5 class="card-title">{{ traobj.title }}</h5>
        <ul class="object-features">
            <li><a href="#" class="badge" style="background-color: {{ traobj.category.color }};"><i class="fa fa-money"></i> {{ traobj.category.label }}</a></li>
            <li><a href="#" class="badge badge-info"><i class="fa fa-calendar"></i>{{ traobj.eventAt|date("d/m/y") }}</a></li>
            <li><a href="#" class="badge badge-info"><i class="fa fa-map-marker"></i> {{ traobj.county.label }}</a></li>
        </ul>
    </div>
    <a href="{{ path('show_traobject',{'id': traobj.id}) }}" class="card-footer-btn btn btn-primary"><i class="fa fa-eye"></i> Afficher</a>

</div>